<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="blog_post_head_handle" />
    <body>
        <referenceContainer name="content">
            <block class="Mageplaza\Blog\Block\Frontend" name="mp.blog.header" template="Mageplaza_Blog::blog/header.phtml"/>
            <block class="Mageplaza\Blog\Block\Category\Listpost" name="mp.blog.category.listpost" template="Mageplaza_Blog::category/post/list.phtml"/>
        </referenceContainer>
    </body>
</page>